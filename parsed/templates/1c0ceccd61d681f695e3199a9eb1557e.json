{
  "components/faceted-search/facets/range": "<div\n  data-facet-filter-wrapper\n  class=\"facet-filter-wrapper {{#unless start_collapsed}}is-open{{/unless}}\"\n>\n  <form\n    class=\"\n      facet-filter-list\n      {{#if selected}}\n        is-open\n      {{/if}}\n      {{#unless start_collapsed}}\n        is-open\n      {{/unless}}\n    \"\n    method=\"get\"\n    data-facet-filter-wrapper\n    data-faceted-search-range\n  >\n\n    {{#each current_selected_items}}\n      <input type=\"hidden\" name=\"{{param_name}}\" value=\"{{param_value}}\"/>\n    {{/each}}\n\n    <div class=\"form-field form-field-range\">\n      <div class=\"form-field-control min\">\n        <input\n          class=\"form-input\"\n          name=\"min_price\"\n          min=\"0\"\n          type=\"number\"\n          value=\"{{min_price}}\"\n          placeholder=\"{{lang 'faceted_search.min_price'}}\"\n          required\n        />\n      </div>\n\n      <div class=\"form-field-control max\">\n        <input\n          class=\"form-input\"\n          name=\"max_price\"\n          min=\"0\"\n          type=\"number\"\n          value=\"{{max_price}}\"\n          placeholder=\"{{lang 'faceted_search.max_price'}}\"\n          required\n        />\n      </div>\n\n      <div class=\"form-actions\">\n        <input\n          class=\"button button-small\"\n          type=\"submit\"\n          value=\"{{lang 'faceted_search.update'}}\"\n        />\n      </div>\n    </div>\n  </form>\n</div>\n"
}