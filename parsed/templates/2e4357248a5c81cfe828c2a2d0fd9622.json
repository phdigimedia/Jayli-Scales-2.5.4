{
  "core/forms/options/set-select": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-select\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"set-select\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <div class=\"form-select-wrapper\">\n        <select\n          class=\"form-input form-select\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\"\n        >\n          <option\n            value=\"\"\n            {{#any values selected=true}}\n            {{else}}\n              selected\n            {{/any}}\n          >\n            {{lang 'product.pick_one'}}\n          </option>\n          {{#each values}}\n            <option\n              value=\"{{id}}\"\n              {{#if selected}}selected{{/if}}\n              data-product-attribute-value=\"{{id}}\"\n            >\n              {{label}}\n            </option>\n          {{/each}}\n          {{#unless required}}\n            <option\n              value=\"{{id}}\"\n              {{#if selected}}selected{{/if}}\n              data-product-attribute-value=\"{{id}}\"\n            >\n              {{lang 'core.product.none'}}\n            </option>\n          {{/unless}}\n        </select>\n      </div>\n    </span>\n  </label>\n</div>\n"
}