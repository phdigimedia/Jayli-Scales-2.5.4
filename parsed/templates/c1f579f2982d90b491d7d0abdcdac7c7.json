{
  "components/wishlist/wishlist-modal": "<div class=\"modal-content\">\n  <h1 class=\"wishlist-modal-title\">\n    {{#if customer.wishlists}}\n      {{lang 'product.wishlist.choose_wishlist'}}\n    {{else}}\n      {{lang 'core.account.wishlists.create'}}\n    {{/if}}\n  </h1>\n\n  {{#if customer.wishlists}}\n    {{> components/wishlist/wishlist-list}}\n  {{/if}}\n  {{> components/wishlist/wishlist-create}}\n</div>\n<div class=\"wishlist-modal-overlay\" data-wishlist-modal-overlay></div>\n<button class=\"quick-shop-close\" data-wishlist-close>\n  <svg><use xlink:href=\"#icon-close\"></svg>\n</button>\n",
  "components/wishlist/wishlist-list": "<form\n  id=\"form-wishlist\"\n  class=\"form wishlist-list-form\"\n  data-wishlist-list-form\n  data-validated-form\n>\n  <div class=\"form-field form-field-options form-field-radio form-required\">\n    <span class=\"form-field-control\">\n      {{#each customer.wishlists}}\n        <label class=\"form-label\">\n          <input class=\"form-input show-for-sr\"\n            id=\"{{name}}\"\n            name=\"product-wishlist\"\n            type=\"radio\"\n            value=\"{{../product.add_to_wishlist_url}}&wishlistid={{id}}\"\n            required\n            aria-required=\"true\"\n          >\n          <span class=\"form-label-text\">\n            {{name}}\n          </span>\n        </label>\n      {{/each}}\n    </span>\n  </div>\n</form>\n",
  "components/wishlist/wishlist-create": "<form\n  class=\"form wishlist-form\"\n  action=\"{{urls.account.wishlists.add}}&product_id={{product.id}}\"\n  method=\"post\"\n  data-validated-form\n  data-wishlist-form\n>\n  <input type=\"hidden\" name=\"submit\">\n\n  <div class=\"wishlist-form-options\">\n    {{> core/forms/common/text\n      required=true\n      name=\"wishlistname\"\n      id=\"wishlistname\"\n      value=forms.wishlist.name\n      prefill=\"New Wishlist\"\n    }}\n\n    <div class=\"form-field form-field-checkbox\">\n      <div class=\"form-field-control\">\n        <label class=\"form-label\">\n          <input\n            class=\"form-checkbox\"\n            name=\"publicwishlist\"\n            type=\"checkbox\"\n            {{#if forms.wishlist.is_public}}checked{{/if}}\n            aria-required=\"false\"\n          >\n          <span class=\"form-label-text\">\n            {{lang 'core.account.wishlists.make_public'}}\n          </span>\n        </label>\n      </div>\n    </div>\n  </div>\n\n  <div class=\"form-actions\">\n    <input\n      type=\"submit\"\n      class=\"button button-primary button-wishlist\"\n      value=\"{{lang 'product.wishlist.add'}}\"\n    >\n  </div>\n</form>\n",
  "core/forms/common/text": "\n<div class=\"form-field form-field-common form-field-text {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n"
}