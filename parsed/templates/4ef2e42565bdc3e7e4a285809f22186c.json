{
  "components/product/product-gallery-slideshow": "<div\n  class=\"\n    product-gallery-slideshow\n    {{#if product.images.length '>' 1}}visible-controls{{/if}}\n  \"\n>\n  <div\n    class=\"product-gallery-slideshow-images\"\n    data-slideshow-position=\"0\"\n    data-product-slideshow\n  >\n    {{#if product.images}}\n      {{#each product.images}}\n        <img\n          class=\"product-gallery-slideshow-image\"\n          data-image-position=\"{{@index}}\"\n          src=\"{{getImage this 'zoom'}}\"\n          alt=\"{{this.alt}}\"\n          data-flickity-lazyload-srcset=\"{{getImage this 'zoom'}}\"\n          data-flickity-lazyload-src=\"{{getImage this 'zoom'}}\"\n          data-product-slideshow-image\n        >\n      {{/each}}\n    {{else}}\n      <img\n        class=\"product-gallery-slideshow-image no-image\"\n        data-image-position=\"{{@index}}\"\n        src=\"{{cdn '/assets/img/no-image.svg'}}\"\n        alt=\"{{lang 'product.no_image'}}\"\n        data-product-slideshow-image\n      >\n    {{/if}}\n  </div>\n\n  <div\n    class=\"\n      product-gallery-slideshow-thumbnail-list\n      {{#if product.images.length \"<\" 2}}\n        product-gallery-slideshow-thumbnail-list-single\n      {{/if}}\n    \"\n  >\n    {{#if product.images}}\n      {{#each product.images}}\n        <span\n          class=\"product-gallery-slideshow-thumbnail {{#if @first}}active{{/if}}\"\n          style=\"background-image: url({{getImage this 'large'}});\"\n          data-image-position=\"{{@index}}\"\n          data-high-res=\"{{getImage this 'large'}}\"\n          data-product-thumbnail\n        >\n          <img\n            class=\"show-for-sr\"\n            src=\"{{getImage this 'small'}}\"\n            alt=\"{{this.alt}}\"\n          >\n        </span>\n      {{/each}}\n    {{else}}\n      <span\n        class=\"product-gallery-slideshow-thumbnail {{#if @first}}active{{/if}}\"\n        style=\"background-image: url({{cdn '/assets/img/no-image.svg'}});\"\n        data-image-position=\"{{@index}}\"\n        data-high-res=\"{{cdn '/assets/img/no-image.svg'}}\"\n        data-product-thumbnail\n      >\n        <img\n          class=\"show-for-sr\"\n          src=\"{{cdn '/assets/img/no-image.svg'}}\"\n          alt=\"{{this.alt}}\"\n        >\n      </span>\n    {{/if}}\n  </div>\n</div>\n"
}