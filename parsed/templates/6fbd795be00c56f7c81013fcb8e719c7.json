{
  "components/faceted-search/facets/hierarchy": "<div\n  data-facet-filter-wrapper\n  class=\"facet-filter-wrapper {{#unless start_collapsed}}is-open{{/unless}}\"\n>\n  <ul\n    class=\"\n      facet-filter-list\n      facet-default\n      {{#if items_count '<' total_items}}has-additional-filters{{/if}}\n    \"\n    id=\"faceted-search-filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n    data-facet=\"{{facet}}\"\n    data-count=\"{{items_count}}\"\n    data-facet-filter-wrapper\n  >\n    {{#each items}}\n      <li {{#if selected}}class=\"facet-selected\"{{/if}} data-facet-item>\n        <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n          {{{title}}}\n\n          {{#if ../show_product_counts}}\n            <span class=\"facet-item-count\" data-facet-item-count>\n              ({{count}})\n            </span>\n          {{/if}}\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  {{#if show_more_toggle}}\n    <a\n      href=\"#faceted-search-filter-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n      role=\"button\"\n      class=\"toggleLink facet-item faceted-show-more-button\n      {{#unless start_collapsed}}is-open{{/unless}}\"\n      data-show-more-facets\n    >\n\n      <span class=\"toggle-link-text toggle-link-text-off\">\n        {{lang 'faceted_search.show_more'}}\n      </span>\n      <span class=\"toggle-link-text toggle-link-text-on\">\n        {{lang 'faceted_search.show_less'}}\n      </span>\n    </a>\n  {{/if}}\n</div>\n"
}